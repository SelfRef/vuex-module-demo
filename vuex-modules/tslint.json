{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [true, "tabs", 2],
    "quotemark": [true, "single"],
    "trailing-comma": [true, "multiline"],
    "ordered-imports": false,
    "interface-name": false
  }
}
